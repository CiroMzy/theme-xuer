
<link rel="canonical" href="{{ canonical_url }}">
<link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
<link rel="preconnect" href="https://monorail-edge.shopifysvc.com">

{%- unless settings.type_header_font.system? and settings.type_base_font.system? -%}
  <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
{%- endunless -%}

{%- if settings.favicon != blank -%}
  <link rel="icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
{%- endif -%} 

{%- unless settings.type_header_font.system? -%}
  <link rel="preload" href="{{ settings.type_header_font | font_url }}" as="font" type="font/woff2" crossorigin>
{%- endunless -%}

{%- unless settings.type_base_font.system? -%}
  <link rel="preload" href="{{ settings.type_base_font | font_url }}" as="font" type="font/woff2" crossorigin>
{%- endunless -%}


